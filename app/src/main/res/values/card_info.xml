<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- This file contains values that are used to display cards. -->

    <!-- Icons for each expansion, sorted by set_id -->
    <array name="card_set_icons">
        <item>@drawable/ic_set_base</item>
        <item>@drawable/ic_set_alchemy</item>
        <item>@drawable/ic_set_black_market</item>
        <item>@drawable/ic_set_cornucopia</item>
        <item>@drawable/ic_set_dark_ages</item>
        <item>@drawable/ic_set_envoy</item>
        <item>@drawable/ic_set_governor</item>
        <item>@drawable/ic_set_guilds</item>
        <item>@drawable/ic_set_hinterlands</item>
        <item>@drawable/ic_set_intrigue</item>
        <item>@drawable/ic_set_prince</item>
        <item>@drawable/ic_set_prosperity</item>
        <item>@drawable/ic_set_seaside</item>
        <item>@drawable/ic_set_stash</item>
        <item>@drawable/ic_set_walled_village</item>
        <item>@drawable/ic_set_adventures</item>
        <item>@drawable/ic_set_summon</item>
        <item>@drawable/ic_set_empires</item>
        <item>@drawable/ic_set_sauna_avanto</item>
    </array>

    <!-- Default translation to use for each set. Sorted by set_id. -->
    <string-array name="def_trans">
        <item>en</item> <!-- 0 Base -->
        <item>en</item> <!-- 1 Alchemy -->
        <item>en</item> <!-- 2 Black Market -->
        <item>en</item> <!-- 3 Cornucopia -->
        <item>en</item> <!-- 4 Dark Ages -->
        <item>en</item> <!-- 5 Envoy -->
        <item>en</item> <!-- 6 Governor -->
        <item>en</item> <!-- 7 Guilds -->
        <item>en</item> <!-- 8 Hinterlands -->
        <item>en</item> <!-- 9 Intrigue -->
        <item>en</item> <!-- 10 Prince -->
        <item>en</item> <!-- 11 Prosperity -->
        <item>en</item> <!-- 12 Seaside -->
        <item>en</item> <!-- 13 Stash -->
        <item>en</item> <!-- 14 Walled Village -->
        <item>en</item> <!-- 15 Adventures -->
        <item>en</item> <!-- 16 Summon -->
        <item>en</item> <!-- 17 Empires -->
        <item>en</item> <!-- 18 Sauna/Avanto -->
    </string-array>

    <string name="coin">Coin</string>
    <plurals name="format_coin">
        <item quantity="one">%1$s Coin</item>
        <item quantity="other">%1$s Coins</item>
    </plurals>

    <string name="debt">Debt</string>
    <plurals name="format_debt">
        <item quantity="one">%1$s Debt</item>
        <item quantity="other">%1$s Debt</item>
    </plurals>

    <string name="potion">Potion</string>

    <string name="vp">Victory Point</string>
    <plurals name="format_vp">
        <item quantity="one">%1$s Victory Point</item>
        <item quantity="other">%1$s Victory Points</item>
    </plurals>

    <string name="card_no_info">This card hasn\'t been translated into English.\n\nDo you own this card? If so, please send a translation to Mark so he can add it in.</string>
    <string name="card_mail_subject">Dominion Card #%1$d (%2$s)</string>

</resources>
